# Liste des fichiers sources
SOURCE=calculg.c calcadd.c calcmult.c

# Liste des fichiers objets
OBJETS=calculg.o calcadd.o calcmult.o

calculg : $(OBJETS)
	cc -o calculg $(OBJETS)

calculg.o : calculg.c
	cc -c calculg.c

calcadd.o : calcadd.c
	cc -c calcadd.c

calcmult.o : calcmult.c
	cc -c calcmult.c

